{
	"semi": true,
	"trailingComma": "all",
	"singleQuote": true,
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"endOfLine": "lf",
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"bracketSameLine": false,
	"proseWrap": "preserve",
	"singleAttributePerLine": true
}
